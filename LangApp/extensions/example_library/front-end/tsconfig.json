{
  "compilerOptions": {
    "target": "es5",
    "outDir": "./dist/",
    "sourceMap": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "typeRoots": ["./node_modules/@types"]  // dodane
  },
  "include": [
    "src"
  ],
  "exclude": ["node_modules"]  // dodane
}


